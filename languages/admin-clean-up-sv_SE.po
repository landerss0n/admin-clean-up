msgstr "Sidor"

#: includes/class-admin-page.php:586
msgid "Hide \"Pages\" menu"
msgstr "Dölj \"Sidor\"-menyn"

#: includes/class-admin-page.php:591
msgid "Appearance"
msgstr "Utseende"

#: includes/class-admin-page.php:598
msgid "Hide \"Appearance\" menu"
msgstr "Dölj \"Utseende\"-menyn"

#: includes/class-admin-page.php:603
msgid "Plugins"
msgstr "Tillägg"

#: includes/class-admin-page.php:610
msgid "Hide \"Plugins\" menu"
msgstr "Dölj \"Tillägg\"-menyn"

#: includes/class-admin-page.php:615
msgid "Users"
msgstr "Användare"

#: includes/class-admin-page.php:622
msgid "Hide \"Users\" menu"
msgstr "Dölj \"Användare\"-menyn"

#: includes/class-admin-page.php:627
msgid "Tools"
msgstr "Verktyg"

#: includes/class-admin-page.php:634
msgid "Hide \"Tools\" menu"
msgstr "Dölj \"Verktyg\"-menyn"

#: includes/class-admin-page.php:639
msgid "Settings"
msgstr "Inställningar"

#: includes/class-admin-page.php:646
msgid "Hide \"Settings\" menu"
msgstr "Dölj \"Inställningar\"-menyn"


#: includes/class-admin-page.php:664
msgid "Footer Settings"
msgstr "Footer-inställningar"

#: includes/class-admin-page.php:666
msgid "Customize or remove the text in the admin footer."
msgstr "Anpassa eller ta bort texten i admin-footern."

#: includes/class-admin-page.php:672
msgid "Footer Text"
msgstr "Footer-text"

#: includes/class-admin-page.php:679
msgid "Remove footer text (\"Thank you for creating with WordPress\")"
msgstr "Ta bort footer-texten (\"Tack för att du skapar med WordPress\")"

#: includes/class-admin-page.php:684
msgid "Custom Footer Text"
msgstr "Egen footer-text"

#: includes/class-admin-page.php:690
msgid "E.g. Developed by Digiwise"
msgstr "T.ex. Utvecklad av Digiwise"

#: includes/class-admin-page.php:692
msgid "Leave empty to use WordPress default text, or enter custom text. Ignored if \"Remove footer text\" is enabled."
msgstr "Lämna tomt för att använda WordPress standardtext, eller ange egen text. Ignoreras om \"Ta bort footer-texten\" är aktiverat."

#: includes/class-admin-page.php:697
msgid "Version Number"
msgstr "Versionsnummer"

#: includes/class-admin-page.php:704
msgid "Remove WordPress version number"
msgstr "Ta bort WordPress-versionsnumret"

#: includes/class-admin-page.php:709
msgid "Custom Version Text"
msgstr "Egen versionstext"

#: includes/class-admin-page.php:715
msgid "E.g. Version 2.0"
msgstr "T.ex. Version 2.0"

#: includes/class-admin-page.php:717
msgid "Leave empty to show WordPress version, or enter custom text. Ignored if \"Remove version number\" is enabled."
msgstr "Lämna tomt för att visa WordPress-versionen, eller ange egen text. Ignoreras om \"Ta bort versionsnumret\" är aktiverat."

#: includes/class-admin-page.php:732
msgid "Notices Settings"
msgstr "Meddelandeinställningar"

#: includes/class-admin-page.php:734
msgid "Control which notices and elements are displayed in admin."
msgstr "Kontrollera vilka meddelanden och element som visas i admin."

#: includes/class-admin-page.php:740
msgid "Update Notices"
msgstr "Uppdateringsmeddelanden"

#: includes/class-admin-page.php:747
msgid "Hide update notices"
msgstr "Dölj uppdateringsmeddelanden"

#: includes/class-admin-page.php:750
msgid "Hides notices about WordPress, plugin and theme updates."
msgstr "Döljer meddelanden om WordPress-, plugin- och temauppdateringar."

#: includes/class-admin-page.php:755
msgid "All Notices"
msgstr "Alla meddelanden"

#: includes/class-admin-page.php:762
msgid "Hide all admin notices"
msgstr "Dölj alla admin-meddelanden"

#: includes/class-admin-page.php:765
msgid "Warning: This hides ALL notices, including important error messages and warnings."
msgstr "Varning: Detta döljer ALLA meddelanden, inklusive viktiga felmeddelanden och varningar."

#: includes/class-admin-page.php:770
msgid "Screen Options"
msgstr "Skärmalternativ"

#: includes/class-admin-page.php:777
msgid "Hide \"Screen Options\" tab"
msgstr "Dölj \"Skärmalternativ\"-fliken"

#: includes/class-admin-page.php:782
msgid "Help Tab"
msgstr "Hjälp-flik"

#: includes/class-admin-page.php:789
msgid "Hide \"Help\" tab"
msgstr "Dölj \"Hjälp\"-fliken"

#: includes/class-admin-page.php:804
msgid "Media Settings"
msgstr "Mediainställningar"

#: includes/class-admin-page.php:806
msgid "Settings for the media library and file uploads."
msgstr "Inställningar för mediabiblioteket och filuppladdningar."

#: includes/class-admin-page.php:812
msgid "Clean Filenames"
msgstr "Rensa filnamn"

#: includes/class-admin-page.php:819
msgid "Clean filenames automatically on upload"
msgstr "Rensa filnamn automatiskt vid uppladdning"

#: includes/class-admin-page.php:822
msgid "This automatically cleans filenames when files are uploaded:"
msgstr "Detta rensar filnamn automatiskt när filer laddas upp:"

#: includes/class-admin-page.php:825
msgid "Converts special characters (å, ä, ö, etc.) to their ASCII equivalents"
msgstr "Konverterar svenska tecken (å, ä, ö) till a, a, o"

#: includes/class-admin-page.php:826
msgid "Replaces spaces and underscores with dashes"
msgstr "Ersätter mellanslag och understreck med bindestreck"

#: includes/class-admin-page.php:827
msgid "Removes special characters and accents"
msgstr "Tar bort specialtecken och accenter"

#: includes/class-admin-page.php:828
msgid "Converts to lowercase"
msgstr "Konverterar till gemener (lowercase)"

#: includes/class-admin-page.php:829
msgid "Preserves the original title as the image title in the media library"
msgstr "Bevarar originaltiteln som bildens titel i mediabiblioteket"

#: includes/class-admin-page.php:832
msgid "Example:"
msgstr "Exempel:"

#: includes/class-admin-page.php:838
msgid "File Types to Clean"
msgstr "Filtyper att rensa"

#: includes/class-admin-page.php:846
msgid "All file types"
msgstr "Alla filtyper"

#: includes/class-admin-page.php:854
msgid "Images only (JPG, PNG, GIF, WebP, etc.)"
msgstr "Endast bilder (JPG, PNG, GIF, WebP, etc.)"

#: includes/class-admin-page.php:858
msgid "Select which file types should have their filenames cleaned on upload."
msgstr "Välj vilka filtyper som ska få sina filnamn rensade vid uppladdning."

#: includes/class-admin-page.php:879
msgid "Plugin Notices Settings"
msgstr "Inställningar för plugin-meddelanden"

#: includes/class-admin-page.php:881
msgid "Hide annoying promotional notices and nag screens from specific plugins."
msgstr "Dölj irriterande reklammeddelanden och tjatskärmar från specifika plugins."

#: includes/class-admin-page.php:888
msgid "PixelYourSite"
msgstr "PixelYourSite"

#: includes/class-admin-page.php:895
msgid "Hide PixelYourSite promotional notices"
msgstr "Dölj PixelYourSite reklammeddelanden"

#: includes/class-admin-page.php:898
msgid "Hides the \"Free PIXELYOURSITE HACKS\" email signup, video tips, and other promotional notices from PixelYourSite plugin."
msgstr "Döljer \"Free PIXELYOURSITE HACKS\" e-postregistrering, videotips och andra reklammeddelanden från PixelYourSite-pluginet."

#: includes/class-admin-page.php:213
msgid "Updates"
msgstr "Uppdateringar"

#: includes/class-admin-page.php:922
msgid "Automatic Updates Settings"
msgstr "Inställningar för automatiska uppdateringar"

#: includes/class-admin-page.php:924
msgid "Control automatic updates for WordPress core, plugins, and themes."
msgstr "Kontrollera automatiska uppdateringar för WordPress-kärnan, plugins och teman."

#: includes/class-admin-page.php:930
msgid "WordPress Core Updates"
msgstr "WordPress kärnuppdateringar"

#: includes/class-admin-page.php:937
msgid "WordPress default (minor updates only)"
msgstr "WordPress standard (endast mindre uppdateringar)"

#: includes/class-admin-page.php:944
msgid "Security updates only (minor releases)"
msgstr "Endast säkerhetsuppdateringar (mindre versioner)"

#: includes/class-admin-page.php:951
msgid "Minor updates only (e.g., 6.4.1 to 6.4.2)"
msgstr "Endast mindre uppdateringar (t.ex. 6.4.1 till 6.4.2)"

#: includes/class-admin-page.php:958
msgid "All updates (major + minor, e.g., 6.4 to 6.5)"
msgstr "Alla uppdateringar (stora + små, t.ex. 6.4 till 6.5)"

#: includes/class-admin-page.php:965
msgid "Disable all automatic core updates"
msgstr "Inaktivera alla automatiska kärnuppdateringar"

#: includes/class-admin-page.php:970
msgid "Note: Security updates are recommended to keep your site safe."
msgstr "Obs: Säkerhetsuppdateringar rekommenderas för att hålla din webbplats säker."

#: includes/class-admin-page.php:975
msgid "Plugin Auto-Updates"
msgstr "Automatiska plugin-uppdateringar"

#: includes/class-admin-page.php:982
msgid "Disable automatic plugin updates"
msgstr "Inaktivera automatiska plugin-uppdateringar"

#: includes/class-admin-page.php:985
msgid "Prevents plugins from updating automatically. You can still update them manually."
msgstr "Förhindrar att plugins uppdateras automatiskt. Du kan fortfarande uppdatera dem manuellt."

#: includes/class-admin-page.php:990
msgid "Theme Auto-Updates"
msgstr "Automatiska temauppdateringar"

#: includes/class-admin-page.php:997
msgid "Disable automatic theme updates"
msgstr "Inaktivera automatiska temauppdateringar"

#: includes/class-admin-page.php:1000
msgid "Prevents themes from updating automatically. You can still update them manually."
msgstr "Förhindrar att teman uppdateras automatiskt. Du kan fortfarande uppdatera dem manuellt."

#: includes/class-admin-page.php:1005
msgid "Update Emails"
msgstr "Uppdateringsmail"

#: includes/class-admin-page.php:1012
msgid "Disable update notification emails"
msgstr "Inaktivera notifikationsmail för uppdateringar"

#: includes/class-admin-page.php:1015
msgid "Stops WordPress from sending emails about automatic updates (core, plugins, themes)."
msgstr "Stoppar WordPress från att skicka e-post om automatiska uppdateringar (kärna, plugins, teman)."

#: includes/class-admin-page.php:1020
msgid "Update Nags"
msgstr "Uppdateringspåminnelser"

#: includes/class-admin-page.php:1027
msgid "Hide update nags in admin"
msgstr "Dölj uppdateringspåminnelser i admin"

#: includes/class-admin-page.php:1030
msgid "Hides the \"WordPress X.X is available! Please update now\" message and update count badges."
msgstr "Döljer meddelandet \"WordPress X.X är tillgänglig! Uppdatera nu\" och uppdateringsräknare."
