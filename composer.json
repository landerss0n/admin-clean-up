{
	"name": "digiwise/admin-clean-up",
	"description": "WordPress plugin that cleans up and simplifies the admin interface",
	"type": "wordpress-plugin",
	"license": "GPL-2.0-or-later",
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^9.6",
		"brain/monkey": "^2.6",
		"mockery/mockery": "^1.6",
		"yoast/phpunit-polyfills": "^2.0"
	},
	"autoload-dev": {
		"psr-4": {
			"AdminCleanUp\\Tests\\": "tests/"
		}
	},
	"scripts": {
		"test": "phpunit",
		"test:unit": "phpunit --testsuite=Unit",
		"test:integration": "phpunit -c phpunit-integration.xml",
		"test:coverage": "phpunit --coverage-html coverage"
	},
	"config": {
		"allow-plugins": {
			"dealerdirect/phpcodesniffer-composer-installer": true
		}
	}
}
